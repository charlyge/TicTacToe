<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tic_tac_toe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VsHumanActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".VsComputerActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".Board3_Vs_Comp"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".Board5_Vs_comp"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".Board3_Vs_Human"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".Board5_vs_human"
            android:configChanges="orientation"
            android:screenOrientation="portrait">


        </activity>
    </application>

</manifest>